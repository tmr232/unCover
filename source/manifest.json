{
  "name": "unCover",
  "version": "1.0",
  "description": "Remove annoying covers from websites",
  
  "permissions" : [
	  "http://gag.bz/*",
	  "http://sofunnylol.org/*"
  ],
  
  "content_scripts": [
    {
      "matches": ["http://gag.bz/*", "http://sofunnylol.org/*"],
      "css": ["gagbz.css"]
    },
	{
		"matches": ["http://www.wolframalpha.com/*"],
		"css": ["wolfram.css"]
	}
  ],

  "manifest_version": 2
}
